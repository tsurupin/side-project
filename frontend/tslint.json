{
    "defaultSeverity": "error",
    "extends": [
        "tslint-config-airbnb",
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "rules": {
        "max-line-length": [true, 120],
        "object-shorthand-properties-first": false,
        "radix": false,
        "ter-arrow-parens": false
    }
}